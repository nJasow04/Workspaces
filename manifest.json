{
    "manifest_version": 3,
    "name": "Workspaces",
    "version": "1.0",
    "permissions": ["tabs", "storage", "windows", "webRequest", "favicon"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "Media/workspace.png"
    },

    "web_accessible_resources": [ 
        { 
            "resources": ["_favicon/*"], 
            "matches": ["<all_urls>"], 
            "extension_ids": ["*"] 
        } 
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["main.js"]
      }
    ]

  }
  