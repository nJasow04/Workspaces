{
    "manifest_version": 2,
    "name": "Workspaces",
    "version": "1.0",
    "permissions": ["tabs", "storage", "windows"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "options_page": "options.html"
  }
  