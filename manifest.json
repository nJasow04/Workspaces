{
    "manifest_version": 3,
    "name": "Workspaces",
    "version": "1.0",
    "permissions": ["tabs", "storage", "windows", "activetab", "webRequest", "webRequestBlocking", "http://", "favicon"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "Media/workspace.png"
    },
    "main_page": "index.html",

    "web_accessible_resources": [ 
        { 
            "resources": ["_favicon/*"], 
            "matches": ["<all_urls>"], 
            "extension_ids": ["*"] 
        } 
    ],

    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["main.js"]
        }
    ]

  }
  